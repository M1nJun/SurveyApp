<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Text?>

<VBox alignment="TOP_CENTER" fillWidth="false" prefHeight="292.0" prefWidth="431.0" spacing="20.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.mycompany.surveyfxml.PrimaryController">
   <children>
      <HBox fillHeight="false" prefHeight="45.0" prefWidth="391.0">
         <children>
            <Label text="Question:">
               <HBox.margin>
                  <Insets left="20.0" right="40.0" />
               </HBox.margin>
            </Label>
            <TextField fx:id="question" prefHeight="26.0" prefWidth="260.0">
               <HBox.margin>
                  <Insets right="10.0" />
               </HBox.margin></TextField>
         </children>
      </HBox>
      <HBox fillHeight="false" prefHeight="12.0" prefWidth="391.0">
         <children>
            <Label text="Options:">
               <HBox.margin>
                  <Insets left="20.0" right="40.0" />
               </HBox.margin>
            </Label>
            <TextField fx:id="op1" prefHeight="26.0" prefWidth="126.0">
               <HBox.margin>
                  <Insets left="6.0" right="10.0" />
               </HBox.margin></TextField>
            <TextField fx:id="op2" prefHeight="26.0" prefWidth="126.0" />
         </children>
      </HBox>
      <HBox fillHeight="false" prefHeight="45.0" prefWidth="391.0">
         <children>
            <TextField fx:id="op3" prefHeight="26.0" prefWidth="126.0">
               <HBox.margin>
                  <Insets left="112.0" right="10.0" />
               </HBox.margin>
            </TextField>
            <TextField fx:id="op4" prefHeight="26.0" prefWidth="126.0" />
         </children>
      </HBox>
      <HBox fillHeight="false" prefHeight="45.0" prefWidth="391.0">
         <children>
            <Label text="Date:">
               <HBox.margin>
                  <Insets left="20.0" right="40.0" />
               </HBox.margin>
            </Label>
            <DatePicker fx:id="datePicker">
               <HBox.margin>
                  <Insets left="23.0" />
               </HBox.margin>
            </DatePicker>
         </children>
      </HBox>
      <HBox alignment="TOP_CENTER" prefHeight="40.0" prefWidth="415.0">
         <children>
            <Button onAction="#addQuestion" prefHeight="27.0" prefWidth="109.0" text="Add Question">
               <HBox.margin>
                  <Insets />
               </HBox.margin>
            </Button>
         </children>
      </HBox>
      <HBox alignment="TOP_CENTER" prefHeight="100.0" prefWidth="200.0">
         <children>
            <Text fx:id="result" strokeType="OUTSIDE" strokeWidth="0.0" />
         </children>
      </HBox>
   </children>
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
</VBox>
